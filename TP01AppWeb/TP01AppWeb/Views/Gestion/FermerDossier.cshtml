@model FermerDossier
@{
    Layout = "_Layout";
}
<main role="main" class="pb-3 logoimage">
    <form asp-action="FermerDossier" method="post">
        <div class="form-group">
            <label asp-for="NoDossier"></label>
            <input  asp-for="NoDossier" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="NoPermis"></label>
            <input  asp-for="NoPermis" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="NoVoiture"></label>
            <input  asp-for="NoVoiture" class="form-control" />
        </div>
        <div asp-validation-summary="All" class="error">

        </div>
        <button type="submit" class="btn btn-primary">Fermer le dossier</button>
    </form>

</main>