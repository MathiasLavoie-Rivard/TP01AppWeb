@model Succursale;
@{
    Layout = "_Layout";
}
<main role="main" class="pb-3 logoimage">
    <form asp-action="Succursale" method="post">
        <div class="form-group">
            <label asp-for="Code"></label>
            <input asp-for="Code" class="form-control" />
            <span asp-validation-for="Code" class="error"></span>
        </div>
        <div class="form-group">
            <label asp-for="NoCivic"></label>
            <input asp-for="NoCivic" class="form-control" />
            <span asp-validation-for="NoCivic" class="error"></span>
        </div>
        <div class="form-group">
            <label asp-for="Rue"></label>
            <input asp-for="Rue" class="form-control" />
            <span asp-validation-for="Rue" class="error"></span>
        </div>
        <div class="form-group">
            <label asp-for="Ville"></label>
            <input asp-for="Ville" class="form-control" />
            <span asp-validation-for="Ville" class="error"></span>
        </div>
        <div class="form-group">
            <label asp-for="Province"></label>
            <input asp-for="Province" class="form-control" />
            <span asp-validation-for="Province" class="error"></span>
        </div>
        <div class="form-group">
            <label asp-for="CodePostal"></label>
            <input asp-for="CodePostal" class="form-control" />
            <span asp-validation-for="CodePostal" class="error"></span>
        </div>
        <div class="form-group">
            <label asp-for="NoTelephone"></label>
            <input asp-for="NoTelephone" class="form-control" />
            <span asp-validation-for="NoTelephone" class="error"></span>
        </div>
        <button type="submit" class="btn btn-primary">Ajouter Succursale</button>
    </form>
</main>